#include "Labyrinthe.h"
#include "Porte.h"
#include "Piece.h"

namespace TP1
{

    Porte::Porte()
    {
    }

    //Constructeur, en argument la couleur de la porte ainsi que la pièce de destination
    Porte::Porte(Couleur c, Piece * d):color(c), destination(d)
    {
    	getCouleur() = color;
    	getDestination() = destination;
    }



    Porte::~Porte()
    {
    }

}
